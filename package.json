{
  "name": "spring-boot-snippets",
  "displayName": "Essential Spring Boot Snippets",
  "description": "High quality Code Snippets that boost your Spring Boot development productivity.",
  "version": "0.5.0",
  "publisher": "doggy8088",
  "icon": "icon.png",
  "preview": true,
  "repository": "https://github.com/doggy8088/spring-boot-snippets",
  "engines": {
    "vscode": "^1.50.0"
  },
  "license": "MIT",
  "galleryBanner": {
    "color": "#DAD0C2",
    "theme": "light"
  },
  "keywords": [
    "spring-boot",
    "java",
    "code-snippets"
  ],
  "categories": [
    "Extension Packs",
    "Snippets"
  ],
  "extensionPack": [
    "vscjava.vscode-java-pack",
    "redhat.fabric8-analytics",
    "redhat.vscode-xml",
    "redhat.vscode-yaml",

    "pivotal.vscode-spring-boot",
    "vscjava.vscode-spring-initializr",
    "vscjava.vscode-spring-boot-dashboard",

    "ms-vscode.live-server",

    "dgileadi.java-decompiler",
    "GabrielBB.vscode-lombok"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "java",
        "path": "./snippets/java.json"
      },
      {
        "language": "editorconfig",
        "path": "./snippets/editorconfig.json"
      },
      {
        "language": "json",
        "path": "./snippets/json.json"
      },
      {
        "language": "jsonc",
        "path": "./snippets/json.json"
      }
    ]
  }
}
