{
    "name": "spring-boot-snippets",
    "displayName": "Essential Spring Boot Snippets",
    "description": "High quality Code Snippets that boost your Spring Boot development productivity.",
    "version": "0.8.5",
    "publisher": "doggy8088",
    "icon": "icon.png",
    "preview": true,
    "repository": "https://github.com/doggy8088/spring-boot-snippets",
    "engines": {
        "vscode": "^1.65.0"
    },
    "license": "MIT",
    "galleryBanner": {
        "color": "#DAD0C2",
        "theme": "light"
    },
    "keywords": [
        "java",
        "spring-boot",
        "code-snippets"
    ],
    "categories": [
        "Extension Packs",
        "Snippets"
    ],
    "extensionPack": [
        "EditorConfig.EditorConfig",
        "vscjava.vscode-java-pack",
        "redhat.fabric8-analytics",
        "redhat.vscode-xml",
        "redhat.vscode-yaml",
        "Pivotal.vscode-boot-dev-pack",
        "ms-vscode.live-server",
        "humao.rest-client",
        "dgileadi.java-decompiler",
        "vscjava.vscode-lombok"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "spring-boot-properties-yaml",
                "path": "./snippets/spring-boot-properties-yaml.json"
            },
            {
                "language": "spring-boot-properties",
                "path": "./snippets/spring-boot-properties.json"
            },
            {
                "language": "java",
                "path": "./snippets/java.json"
            },
            {
                "language": "editorconfig",
                "path": "./snippets/editorconfig.json"
            },
            {
                "language": "json",
                "path": "./snippets/json.json"
            },
            {
                "language": "jsonc",
                "path": "./snippets/json.json"
            }
        ]
    }
}
